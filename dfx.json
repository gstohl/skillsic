{
  "canisters": {
    "skillsic_backend": {
      "type": "rust",
      "candid": "canister/src/skillsic_backend.did",
      "package": "skillsic_backend"
    },
    "skillsic_frontend": {
      "type": "assets",
      "source": ["frontend/dist"],
      "dependencies": ["skillsic_backend"]
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  },
  "output_env_file": ".env",
  "version": 1
}
